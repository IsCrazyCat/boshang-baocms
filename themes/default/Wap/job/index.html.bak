        $('#refer-ad').css('display','none')
    }

    function goNPP() {
        var customer= $("#customer").val();
        if ( customer == null || customer == undefined || customer == "") {
            window.location.href="/login";
        }else{
            window.location.href="/packageRecordDetail";
        }

    }

    function goLogin() {
        window.location.href="/login";
    }

    function focusSearch() {
        window.preventEjectMpQrcode = true;
        window.preventEjectBindMobile = true;
        $('#eject-mp-qrcode').css('display', 'none');
        $('#page-index').removeClass('page-current');
        $('#page-search').addClass('page-current');
        var elem = document.getElementById('search');
        elem.focus();
    }

    function cancelSearch() {
        var elem = document.getElementById('search');
        elem.blur();
        window.preventEjectMpQrcode = false;
        window.preventEjectBindMobile = false;
        $('#page-search').removeClass('page-current');
        $('#page-index').addClass('page-current');
    }
</script>
